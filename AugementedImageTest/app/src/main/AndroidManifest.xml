<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asatkee1.augementedimagetest">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:name="android.hardware.camera.ar"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.ar.core"
            android:value="required" />

        <activity
            android:name=".Initial_Page_Activity"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>

        <activity android:name=".General_Information_Activity"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>

        <!-- R Building -->
        <activity android:name=".RBuilding"
            android:theme="@style/Theme.AppCompat.Translucent"/>

        <activity android:name=".RArtsAndHumanitiesMain"
            android:theme="@style/Theme.AppCompat.Translucent"/>

        <activity android:name=".RArtsAndHumanitiesDepartments"
            android:theme="@style/Theme.AppCompat.Translucent"/>

        <!-- S building -->
        <activity android:name=".SBuilding"
            android:theme="@style/Theme.AppCompat.Translucent"/>

        <activity android:name=".SScienceDepartments"
            android:theme="@style/Theme.AppCompat.Translucent"/>

        <!-- L building -->
        <activity
            android:name=".L_building"
            android:launchMode="singleTop"
            android:theme="@style/Theme.AppCompat.Translucent" />

        <activity
            android:name=".InteriorDesignActivity"
            android:theme="@style/Theme.AppCompat.Translucent"/>

        <activity
            android:name=".ScienceDivisionActivity"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>

        <!-- E building -->
        <activity
            android:name=".EBuilding"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>

        <!-- F building -->
        <activity
            android:name=".FBuilding"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>

        <!-- G building -->
        <activity
            android:name=".GBuilding"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>

        <!-- K building -->
        <activity
            android:name=".KBuilding"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>

        <!-- Q building -->
        <activity
            android:name=".QBuilding"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>

        <activity
            android:name=".QEarlyLearningTeacherEdMain"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>

        <activity
            android:name=".QEarlyLearningCenterMain"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>

        <!-- U building -->
        <activity
            android:name=".UBuilding"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>

    </application>


</manifest>