<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asatkee1.augementedimagetest">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:name="android.hardware.camera.ar"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.ar.core"
            android:value="required" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity" />

        <activity
            android:name=".Initial_Page_Activity"
            android:theme="@style/Theme.AppCompat.Translucent"></activity>
        <activity android:name=".General_Information_Activity"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>
        <!-- R Building -->
        <activity android:name=".RBuilding"
            android:parentActivityName="MainActivity"
            android:theme="@style/Theme.AppCompat.Translucent"/>
        <activity android:name=".RArtsAndHumanitiesMain"
            android:parentActivityName="MainActivity"
            android:theme="@style/Theme.AppCompat.Translucent"/>
        <activity android:name=".RArtsAndHumanitiesDepartments"
            android:parentActivityName="MainActivity"
            android:theme="@style/Theme.AppCompat.Translucent"/>
        <!-- S building -->
        <activity android:name=".SBuilding"
            android:parentActivityName="MainActivity"
            android:theme="@style/Theme.AppCompat.Translucent"/>
        <activity android:name=".SScienceDepartments"
            android:parentActivityName="MainActivity"
            android:theme="@style/Theme.AppCompat.Translucent"/>

    </application>

</manifest>